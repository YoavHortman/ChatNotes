{
  "name": "chat-notes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/react-relay": "1.3.6",
    "graphql": "14.0.0-rc.1",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-relay": "1.6.0",
    "react-scripts-ts": "2.16.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "relay": "relay-compiler --src ./src --schema ./schema.graphql",
    "relay-jsx": "relay-compiler --src ./src --schema ./schema.graphql --extensions js jsx",
    "relay-tsx": "relay-compiler --src ./src --schema ./schema.graphql --extensions ts tsx"

  },
  "devDependencies": {
    "@types/jest": "23.0.0",
    "@types/node": "10.3.1",
    "@types/react": "16.3.16",
    "@types/react-dom": "16.0.6",
    "typescript": "2.9.1"
  }
}
